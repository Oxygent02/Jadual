        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- MAHASISWA -->
        <activity
            android:name=".mahasiswaActivity.MainActivity_Mahasiswa"
            android:label="Jadwal Dosen" />
        <activity android:name=".mahasiswaActivity.JadwalDosenActivity_Mahasiswa" />

        <!-- DOSEN -->
        <activity android:name=".dosenActivity.MainActivity_Dosen" />
        <activity android:name=".dosenActivity.InputJadwalActivity_Dosen"></activity>
    </application>

</manifest>